  "bugs": {
    "url": "https://github.com/hackariens/sveltejs/issues"
  },
  "homepage": "https://github.com/hackariens/sveltejs#readme",
  "dependencies": {
    "create-svelte": "^2.3.2",
    "korojscommands": "^1.2.14"
  }
}
